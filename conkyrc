out_to_console yes
out_to_x no
background no
update_interval 1
total_run_times 0
use_spacer none

TEXT
${cpu cpu1}% ${cpu cpu2}% ${cpu cpu3}% ${cpu cpu4}% - CPU ${exec sensors | grep '.*Core 0.*' | cut -d' ' -f11 | cut -d'.' -f1}C ${exec sensors | grep '.*Core 1.*' | cut -d' ' -f11 | cut -d'.' -f1}C ${exec sensors | grep '.*Core 2.*' | cut -d' ' -f11 | cut -d'.' -f1}C ${exec sensors | grep '.*Core 3.*' | cut -d' ' -f11 | cut -d'.' -f1}C (crit+72.6C) - GPU +${exec nvidia-settings -q gpucoretemp | head -n 2 | tail -n 1 | cut -d' ' -f6 | cut -d'.' -f1}C (crit+100C) - Vol SC ${exec amixer -c 0 sget 'Master',0 | grep '.*Front Right:.*' | grep -o '[0-9]*[0-9]%'} - Vol OB ${exec amixer -c 1 sget 'Master',0 | grep '.*Mono:.*' | grep -o '[0-9]*[0-9]%'} - RAM $memperc% $mem/$memmax - swap $swapperc% $swap/$swapmax - down ${downspeedf enp6s0}KiB/s - up ${upspeedf enp6s0}KiB/s - uptime $uptime - ${time %d-%m-%Y %H:%M:%S}
